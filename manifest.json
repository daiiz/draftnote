{
  "name": "Draftnote",
  "description": "Draftnoteは、シンプルなメモ帳です。最新バージョンでは、機能とアイコンがスマートになりました。",
  "version": "2015.5.10.1112",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {
    "48":  "icon.png",
    "16":  "icon.png",
    "128": "icon.png"
  },
  "permissions": ["storage", "unlimitedStorage", "<all_urls>"]
}
